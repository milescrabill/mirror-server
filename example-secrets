# template for secrets file

export MIRROR_FORECASTTOKEN=forecast.io token goes here
export MIRROR_PLAIDCLIENTID=plaid client id goes here
export MIRROR_PLAIDSECRET=plaid secret goes here
export MIRROR_SECURECOOKIEHASHKEY=generate w/ openssl rand -hex 12 or similar
export MIRROR_SECURECOOKIEBLOCKKEY=generate w/ openssl rand -hex 12 or similar
export MIRROR_NEWSTOKEN=your newsapi.org key
